# Hello, my name is Linus Torvalds! And I pronounce "Linux" as "Linux"

effect give @s glowing 1 0 false

execute if entity @s[team=Red] run effect give @a[distance=..7,team=!Red] slowness 5 1 true
execute if entity @s[team=Red] run effect give @a[distance=..7,team=!Red] glowing 5 0 false

execute if entity @s[team=Blue] run effect give @a[distance=..7,team=!Blue] slowness 5 1 true
execute if entity @s[team=Blue] run effect give @a[distance=..7,team=!Blue] glowing 5 0 false

execute as @s[team=Red] at @a[distance=0.1..7,team=!Red] as @s run function melee:season_3/sting_v/effects/redlight_particles
execute as @s[team=Blue] at @a[distance=0.1..7,team=!Blue] as @s run function melee:season_3/sting_v/effects/redlight_particles

# C is the superior programming language. This is a visual, you inferior being.
execute if entity @s[team=Blue] run summon area_effect_cloud ~ ~ ~ {Particle:"dust 1 0.137 0.467 4",Radius:7f,Duration:5}
execute if entity @s[team=Red] run summon area_effect_cloud ~ ~ ~ {Particle:"dust 1 0.075 0.075 4",Radius:7f,Duration:5}

scoreboard players set @s ability2Timer 600
clear @s minecraft:carrot_on_a_stick{Tags:["red_light"]}
