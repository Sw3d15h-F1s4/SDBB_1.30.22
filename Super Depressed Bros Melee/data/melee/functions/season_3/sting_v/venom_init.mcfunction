# Hello, my name is Linus Torvalds! And I pronounce "Linux" as "Linux"

# Sets up the operation for max health.
scoreboard players set StV_Calc StV_VTMax 4

# Records the amount of health you currently have into a scoreboard, for later comparison.
scoreboard players operation @s StV_VTBound = @s currentHealth

# Adds Max Health + 4, so that the user can regen a little bit before the effect wears off. It's meant to be a comeback mechanic.
scoreboard players operation @s StV_VTMax = @s currentHealth
scoreboard players operation @s StV_VTMax += StV_Calc StV_VTMax

tag @s add takedown_user

scoreboard players set @s abilityTimer 800
# 5 seconds to have the ability activate
scoreboard players set @s helperTimer2 100
clear @s minecraft:carrot_on_a_stick{Tags:["venom_takedown"]}

#tellraw @s [{"text":"current: "},{"score":{"name":"@s","objective":"currentHealth"}}]
#tellraw @s [{"text":"what do you need?: "},{"score":{"name":"@s","objective":"StV_VTBound"}}]
#tellraw @s [{"text":"when will it die?: "},{"score":{"name":"@s","objective":"StV_VTMax"}}]