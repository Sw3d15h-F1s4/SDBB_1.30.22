# Hello, my name is Linus Torvalds! And I pronounce "Linux" as "Linux"

tag @s remove takedown_user
tag @s add takedown_use_success

# One tick of invulnerability(?)
effect give @s resistance 1 255 true

effect give @s glowing 1 1 false
effect clear @s slowness
effect clear @s blindness
effect clear @s mining_fatigue
effect clear @s resistance

execute at @s[team=Red] at @a[team=!Red,distance=..5] run particle minecraft:sweep_attack ~ ~ ~ 0.18 0.7 0.18 1 4 force
execute at @s[team=Blue] at @a[team=!Blue,distance=..5] run particle minecraft:sweep_attack ~ ~ ~ 0.18 0.7 0.18 1 4 force

execute at @s[team=Red] run effect give @a[team=!Red,distance=..5] poison 5 1 true
execute at @s[team=Blue] run effect give @a[team=!Blue,distance=..5] poison 5 1 true

particle minecraft:crit ^ ^0.1 ^ 0.12 0.8 0.12 1 12 force
execute at @s[team=Blue] run summon area_effect_cloud ~ ~ ~ {Particle:"dust 0.271 0.510 0.169 7",Radius:5f}
execute at @s[team=Red] run summon area_effect_cloud ~ ~ ~ {Particle:"dust 0.435 0.588 0.204 7",Radius:5f}

scoreboard players reset @s StV_VTBound
scoreboard players reset @s StV_VTMax
scoreboard players operation @s abilityTimer += @s helperTimer2
scoreboard players set @s helperTimer2 60